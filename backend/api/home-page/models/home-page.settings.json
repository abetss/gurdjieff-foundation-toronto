{
  "kind": "singleType",
  "collectionName": "home_pages",
  "info": {
    "name": "Home page"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "quote": {
      "type": "string"
    },
    "content": {
      "type": "richtext"
    },
    "contentImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "Links": {
      "type": "richtext"
    },
    "Extra_content": {
      "type": "dynamiczone",
      "components": [
        "contact-info.contact",
        "content.cenetered-content",
        "content.default-content",
        "content.image-left-content",
        "content.image-right-content"
      ]
    }
  }
}
